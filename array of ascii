#include <stdio.h>  
#include<iostream>
#include<string.h>
using namespace std;
int main()  
{  
   char name[5];
    cin>>name;
    int i=0;
    int l=0;
    char b=0;
    int  d =0;
    l = sizeof(name)/sizeof(name[0]);
    for(i=0;i<l;++i)
    {
       d= name[i]+5;
        b=d;
       cout<<b;
       d=0;
       
    }
    
}
