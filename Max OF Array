#include<iostream>
using namespace std;
int main()
{
    int max;
    int n;
    cin>>n;
    int arr[n];
    cout<<"Enter the array";
    
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    max=arr[0];
    for(int i=0;i<n;i++)
    {
        if(max<arr[i])
        {
            max=arr[i];
            
        }
        
    }
    cout<<"max no is:"<<max;
}
