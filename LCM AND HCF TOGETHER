#include<iostream>
using namespace std;
int main()
{
    int a,b,r,lcm,hcf;
    cin>>a>>b;
    
    while(true)
    {
        lcm=a;
        r=a%b;
        
        
        if(r==0)
        {
            break;
        }
        a=a*2;  
        
    }
    hcf=(a*b)/lcm;
        cout<<"lcm is "<<lcm;
        cout<<"hcf is "<<hcf;
        
    }
    
